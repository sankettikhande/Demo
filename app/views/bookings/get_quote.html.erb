<div class="result-page">
  <div class="panel result-heading">
      <div class="row">
        <div class="col-xs-2">
          <div class="btn-group frmto-drpdn">
            <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown"><i class="fa fa-map-marker"></i> From - To <span class="caret"></span> <span class="frmto-count">(<%= session['booking_ids'].present? ? session['booking_ids'].count : 1 %>)</span></button>
            <ul class="dropdown-menu frmto-menu" role="menu">
              <% (session['booking_ids'] || [@draft_booking.id]).each do |booking_id| %>
                <% booking = Booking.find(booking_id)%>
                <li class="loc-item" id='booking_<%= booking_id%>'>
                  <div class="loc-item-in">
                    <%= link_to get_quote_booking_path(id: booking.id), class: 'loc-item-link' do %>
                      <span class="loc-pos"><%= booking.source%></span>
                      <span class="loc-pos"><%= booking.destination%></span>
                    <%end%>
                    <button class="btn btn-danger btn-del" type="button" onclick="remove_booking(<%= booking_id%>, this)">&times;</button>
                  </div>    
                </li>
              <%end%>  
            </ul>
          </div>  
        </div>
        <div class="col-xs-7">
          <ul class="list-inline order-label-heading">
            <li class="col-xs-2">
              <span class="ol-lab">
                CBM
              </span>
              <span class="ol-val">
                <%= @draft_booking.cbm%>
              </span>
            </li>
            <li class="col-xs-2">
              <span class="ol-lab">
                Weight
              </span>
              <span class="ol-val">
                <%= @draft_booking.cbm%>kg
              </span>
            </li>
            <li class="col-xs-2">
              <span class="ol-lab">
                Date
              </span>
              <span class="ol-val">
                <%= @draft_booking.pick_up_date %>
              </span>
            </li>
            <li class="col-xs-2">
              <span class="ol-lab">
                Min
              </span>
              <span class="ol-val">
                <%= @minimum_freight_price%>
              </span>
            </li>
            <li class="col-xs-2">
              <span class="ol-lab">
                Average
              </span>
              <span class="ol-val">
                <%= @average_price %>
              </span>
            </li>
            <li class="col-xs-2">
              <span class="ol-lab">
                Fastest
              </span>
              <span class="ol-val">
                <%= @fastest_delivery%>
              </span>
            </li>
          </ul>
        </div>
        <div class="col-xs-3">
          <ul class="list-inline order-sort">

            <li class="col-xs-6"><button type="button" class="btn btn-sm btn-block btn-warning" title="Download" data-toggle="tooltip" data-placement="top">Download</button></li>

            <li class="col-xs-6"><button type="button" class="btn btn-sm btn-block btn-warning" title="Eligible for Discount" data-toggle="tooltip" data-placement="top">With Discount</button></li>

            <li class="col-xs-6"><button type="button" class="btn btn-sm btn-block btn-warning" title="Total Minutes" data-toggle="tooltip" data-placement="top">Total Minutes</button></li>

            <li class="col-xs-6"><button type="button" class="btn btn-sm btn-block btn-warning" title="Total Fastest" data-toggle="tooltip" data-placement="top">Total Fastest</button></li>

          </ul>  
        </div>
      </div>
  </div>

  <div class="result-content">
    <div class="row">
      <div class="col-xs-9 col-xs-offset-1">
        <div class="row">
          <div class="col-xs-12">
            <div class="rlist-wrapper">
              <ul class="rlist-headings">
                <li>&nbsp;</li>
                <li>Ratings</li>
                <li>Cut Offs</li>
                <li>Delivery</li>
                <li>Days</li>
                <li>All - in Price</li>
                <li>&nbsp;</li>
              </ul>

              <% @freight_rates.each do |freight_rate|%>
                <div class="result-item">
                  <div class="rb-item rb-name"><%=image_tag 'dhl.png'%></div>
                  <div class="rb-item rb-rating">
                    <span class="rview-rating"></span> <span class="rview-rating-c">(10)</span>
                  </div>              
                  <div class="rb-item rb-coff"><%= freight_rate.cut_off_date.strftime("%-d, %b")%></div>
                  <div class="rb-item rb-del"><%= delivery_date(freight_rate).strftime("%-d, %b")%></div>
                  <div class="rb-item rb-day"><%= freight_rate.transition_days%></div>
                  <div class="rb-item rb-aip">US $<%= freight_rate.price %></div>
                  <div class="rb-item rb-buk">
                    <button style="display: <%= is_in_cart?(@draft_booking, freight_rate) ? 'block' : 'none' %>" class="btn btn-success selected" id = "selected_freight_<%=  freight_rate.id%>" freight_id = "<%= freight_rate.id %>" booking_id="<%= @draft_booking.id%>" type="button" onclick="remove_from_cart(<%= @draft_booking.id%>, <%= freight_rate.id%>);">Selected</button>

                    <button style="display: <%= !is_in_cart?(@draft_booking, freight_rate) ? 'block' : 'none' %>" class="btn btn-success select" id = "freight_<%=  freight_rate.id%>" freight_id = "<%= freight_rate.id %>" booking_id="<%= @draft_booking.id%>" type="button" onclick="add_to_cart(<%= @draft_booking.id%>, <%= freight_rate.id%>);">Select</button>
                  </div>
                </div>
              <%end%>
            </div>  
          </div>
        </div>
        <div class="row">
          <div class="col-xs-8 col-xs-offset-2">
            <div class="form-group text-center">
              <%= link_to 'Book Now', 'javascript:void()', onclick: "check_cart('/address_books/add_supplier_consignee')", class: "btn btn-primary"%>
              <%= link_to 'javascript:void()', class: "btn btn-default pull-right", onclick: "check_cart('/bookings/booking_summary')" do %>
                View Summary
                <i class="fa fa-angle-double-right"></i>  
              <% end %>
            </div>
          </div>
        </div>  
      </div>  
      <div class="col-xs-2">
        <div class="slctd-sr-list cart_detail">
          <%= render :partial => 'bookings/cart_details'%> 
        </div>           
      
        <div class="ssdd-btn btn-group">
         <a href="#" class="btn btn-default dropdown" data-toggle="dropdown">
           Selected Shippers <span class="caret"></span>
         </a>
         <ul class="dropdown-menu ss-dd-list" role="menu">
           <li class="dd-li">
               <label for="checkbox_1">One</label>
               <input type="checkbox" id="checkbox_1" name="checkbox_1" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_2">Two</label>
               <input type="checkbox" id="checkbox_2" name="checkbox_2" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_3">Three</label>
               <input type="checkbox" id="checkbox_3" name="checkbox_3" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_4">Four</label>
               <input type="checkbox" id="checkbox_4" name="checkbox_4" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_5">One</label>
               <input type="checkbox" id="checkbox_5" name="checkbox_5" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_6">Two</label>
               <input type="checkbox" id="checkbox_6" name="checkbox_6" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_7">Three</label>
               <input type="checkbox" id="checkbox_7" name="checkbox_7" value="Checkbox One" />
           </li>
           <li class="dd-li">
               <label for="checkbox_8">Four</label>
               <input type="checkbox" id="checkbox_8" name="checkbox_8" value="Checkbox One" />
           </li>                     
         </ul>
        </div>

      </div>
    </div>
  </div>
</div>